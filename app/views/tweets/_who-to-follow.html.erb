						<p class="image is-32x32">
							<%= gravatar_image_tag(randomuser.user.email, alt: randomuser.user.email)%>
						</p>
					</figure>
					<div class="media-content">
						<strong><%= randomuser.user.name %></strong>
						<small>@<%= randomuser.user.name %></small>
					</div>
				</div>
			</div>
		<% end %>
