<div class="column">
	<nav class="panel">
		<p class="panel-heading" style="text-align: center; font-weight: bold;">Who to Follow <i class="fa fa-user-plus" style="color: #42C7F9;"></i>	 </p>
		<% @users.each do |randomuser| %>
			<div class="panel-block">
				<div class="media">
					<figure class="media-left">
						<p class="image is-32x32">
							<%= gravatar_image_tag(randomuser.email, alt: randomuser.email)%>
						</p>
					</figure>
					<div class="media-content">
						<strong><%= randomuser.name %></strong>
						<small>@<%= randomuser.name %></small>
					</div>
				</div>
			</div>
		<% end %>
	</nav>
